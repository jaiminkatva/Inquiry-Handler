<div class="content-wrapper">
  <% if(message){ %>
  <div style="z-index: 5" class="fixedbutton p-1 rounded-3">
    <div id="message" class="alert alert-<%= message.type %> alert-dismissible">
      <button
        type="button"
        class="close"
        data-dismiss="alert"
        aria-hidden="true"
      >
        &times;
      </button>
      <h5><i class="icon fas fa-check"></i> Alert!</h5>
      <%= message.message %>
    </div>
  </div>
  <% } %>